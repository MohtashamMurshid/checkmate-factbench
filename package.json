{
  "name": "checkmate-factbench",
  "version": "0.1.0",
  "description": "A factbench library built with Bun",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun run clean && bun build ./index.ts --target node --format esm --minify --outfile ./dist/index.js && bun build ./index.ts --target node --format cjs --minify --outfile ./dist/index.cjs && tsc --emitDeclarationOnly --declaration --declarationMap --outDir dist",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "clean": "rm -rf dist && mkdir -p dist"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "ai": "^5.0.0",
    "@openrouter/ai-sdk-provider": "^0.0.5",
    "@opentui/core": "^0.1.60",
    "react": "^19.0.0",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "string-similarity": "^4.0.4",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
